{
	"name": "obsidian-embed-line-range",
	"version": "3.0.1",
	"description": "This Obsidian plugin brings line range embed and link support to Obsidian. Based on original work by Ryan Mellmer and Eric Sowell.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"postinstall": "patch-package"
	},
	"keywords": [],
	"author": "ctruett",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"patch-package": "^6.5.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"@codemirror/language": "6.10.1"
	},
	"dependencies": {
		"@types/codemirror": "^5.60.7",
		"codemirror": "^6.0.1",
		"monkey-around": "^2.3.0"
	}
}
